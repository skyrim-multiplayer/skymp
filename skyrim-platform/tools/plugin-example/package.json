{
  "name": "example-skyrim-platform-plugin",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "webpack": "webpack",
    "webpack:deploy": "set DEPLOY_PLUGIN=true && npm run webpack",
