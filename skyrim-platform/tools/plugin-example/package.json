    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "keywords": [
    "skyrim",
    "skryim platform"
  ],
  "dependencies": {
    "@skyrim-platform/skyrim-platform": "2.8.0"
  }
}
